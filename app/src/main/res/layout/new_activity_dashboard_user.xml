<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <!-- Activity Dashboard layout -->


    <FrameLayout
        android:id="@+id/moviesFragmentContainer"
        android:layout_width="405dp"
        android:layout_height="250dp"
        android:layout_marginEnd="4dp"
        android:layout_weight="1"
        android:elevation="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="16dp" />

    <FrameLayout
        android:id="@+id/songsFragmentContainer"
        android:layout_width="match_parent"
        android:layout_height="175dp"
        android:layout_marginTop="0dp"
        android:layout_weight="1"
        android:elevation="10dp"
        app:layout_constraintTop_toBottomOf="@+id/moviesFragmentContainer"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="2dp" />


    <FrameLayout
        android:id="@+id/booksFragmentContainer"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_marginTop="0dp"
        android:layout_weight="1"
        android:elevation="10dp"
        app:layout_constraintTop_toBottomOf="@+id/songsFragmentContainer"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="2dp" />

    <include
        android:id="@+id/includedLayout"
        layout="@layout/activity_dashboard_user"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />



</androidx.constraintlayout.widget.ConstraintLayout>

